{
  "name": "ls-contentful-app",
  "version": "1.0.6",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.39.0",
    "@contentful/f36-components": "4.81.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "11.13.5",
    "contentful-management": "10.46.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superagent": "^10.2.3"
  },
  "scripts": {
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "install-ci": "npm ci",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3kMGvoFeVdMPIkolBPdPIg --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.5",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@tsconfig/create-react-app": "2.0.6",
    "@types/jest": "29.5.14",
    "@types/node": "^20",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/superagent": "^8.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "cross-env": "10.0.0",
    "typescript": "5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "homepage": "."
}
