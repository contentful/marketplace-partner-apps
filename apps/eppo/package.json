    "@dnd-kit/sortable": "^8.0.0",
    "@popperjs/core": "^2.11.5",
    "@tanstack/react-query": "^4.3.9",
    "constate": "^3.3.3",
    "contentful-management": "^11.53.0",
    "emotion": "^10.0.17",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.15",
    "moment": "^2.20.0",
    "p-queue": "^4.0.0",
    "react": "18.3.1",
