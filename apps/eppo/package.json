{
  "name": "eppo-contentful-app",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none NODE_ENV=development PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "ci": "lint tsc test",
    "install-ci": "npm ci",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 7EFM8G3tm6fJwrsPRL0VpW --token ${CONTENTFUL_CMA_TOKEN}",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.29.7",
