{
  "name": "global-find-replace",
  "version": "0.2.4",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.51.0",
    "@contentful/f36-components": "^4.81.1",
    "@contentful/f36-icons": "^4.29.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@contentful/rich-text-plain-text-renderer": "^17.1.6",
    "@emotion/css": "^11.13.5",
    "@sentry/react": "^10.34.0",
