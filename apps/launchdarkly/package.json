{
  "name": "launchdarkly-contentful-app",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:9050",
  "scripts": {
    "start": "cross-env BROWSER=none NODE_ENV=development PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "install-ci": "npm ci",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id ${CONTENTFUL_APP_DEF_ID} --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.41.2",
    "@contentful/f36-components": "^4.81.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "11.54.4",
    "emotion": "^10.0.17",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-scripts": "5.0.1",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.5",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@tsconfig/create-react-app": "2.0.6",
    "@types/node": "^20",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "cross-env": "7.0.3",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
