{
  "name": "launchdarkly-contentful-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "install-ci": "npm install",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3xJRrW0VKT07cHgalFjHOi --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.41.2",
    "@contentful/f36-components": "^5.6.0",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "11.62.0",
    "emotion": "^10.0.17",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.6",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.0.3",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.12",
    "vitest": "^3.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
