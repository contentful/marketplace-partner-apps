{
  "name": "amplitude-contentful",
  "version": "1.1.4",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.39.0",
    "@contentful/f36-components": "4.81.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "^11.13.5",
    "contentful-management": "11.54.4",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "proxy": "https://experiment.amplitude.com",
  "scripts": {
    "install-ci": "npm ci",
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3bzEoyxboiWITYoHDd97IE --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.5",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "happy-dom": "^17.2.2",
    "typescript": "5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "homepage": "."
}
