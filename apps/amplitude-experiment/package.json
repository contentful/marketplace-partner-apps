{
  "name": "amplitude-contentful",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.0",
    "@contentful/f36-components": "4.58.4",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "^11.13.0",
    "contentful-management": "10.46.4",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "proxy": "https://experiment.amplitude.com",
  "scripts": {
    "install-ci": "npm ci",
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3bzEoyxboiWITYoHDd97IE --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.25.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "^14.1.2",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.15.3",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "happy-dom": "^14.12.3",
    "typescript": "4.9.5",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "homepage": "."
}
