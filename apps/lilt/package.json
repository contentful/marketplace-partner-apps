{
  "name": "contentful-lilt-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "contentful-extension-scripts start",
    "build": "contentful-extension-scripts build",
    "lint": "eslint ./src ./test --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint ./src ./test --ext .js,.jsx,.ts,.tsx --fix",
    "lint-fix-with-path": "eslint $npm_config_path --ext .js,.jsx,.ts,.tsx --fix",
    "test": "npm run lint && contentful-extension-scripts test --env=jsdom",
    "test:coverage": "contentful-extension-scripts test --env=jsdom --coverage",
    "configure": "contentful space use && contentful space environment use",
    "login": "contentful login",
    "logout": "contentful logout",
    "help": "contentful-extension-scripts help",
    "install-ci": "npm ci"
  },
  "dependencies": {
    "@contentful/f36-components": "^4.9.0",
    "@contentful/forma-36-fcss": "0.0.20",
    "@contentful/forma-36-react-components": "3.37.3",
    "@contentful/rich-text-from-markdown": "^15.16.8",
    "axios": "0.19.2",
    "contentful-management": "^10.0.0",
    "contentful-ui-extensions-sdk": "4.3.5",
    "dotenv": "^16.3.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@contentful/browserslist-config": "^2.0.0",
    "@contentful/contentful-extension-scripts": "0.16.0",
    "@contentful/eslint-config-extension": "0.3.1",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "10.0.4",
    "contentful-cli": "0.33.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
}
