{
  "name": "defaults",
  "version": "0.4.0",
  "dependencies": {
    "@contentful/app-sdk": "^4.29.7",
    "@contentful/f36-components": "4.80.4",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "11.52.2",
    "emotion": "10.0.27",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zustand": "^5.0.5"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && npm run build:functions",
    "test": "vitest run",
    "install-ci": "npm ci",
    "coverage": "vitest run --coverage",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "add-locations": "contentful-app-scripts add-locations",
    "open-settings": "contentful-app-scripts open-settings",
    "install-app": "contentful-app-scripts install",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:functions": "contentful-app-scripts build-functions --ci",
    "create-app-event": "tsx -r dotenv/config ./src/tools/create-app-event.ts",
    "create-content-type-publish-entries": "tsx -r dotenv/config ./src/tools/create-content-type-publish-entries.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "^2.5.5",
    "@contentful/node-apps-toolkit": "^3.13.1",
    "@testing-library/react": "^14.3.1",
    "@tsconfig/recommended": "1.0.8",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.13",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.3",
    "cross-env": "7.0.3",
    "dotenv": "^16.5.0",
    "happy-dom": "17.4.7",
    "tsx": "^4.19.3",
    "typescript": "4.9.5",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "homepage": "."
}
