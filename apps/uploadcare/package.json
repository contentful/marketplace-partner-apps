{
  "name": "uploadcare-contentful-app",
  "version": "1.0.4",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.22.1",
    "@contentful/f36-components": "4.58.1",
    "@contentful/f36-icons": "4.27.0",
    "@contentful/f36-tokens": "4.0.3",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.2",
    "@uploadcare/blocks": "0.30.8",
    "@uploadcare/upload-client": "6.12.1",
    "contentful-management": "10.46.4",
    "emotion": "10.0.27",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "install-ci": "npm ci",
    "lint": "eslint --cache src/**/*.{ts,tsx} && tsc --noEmit",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3Ju8DSs6cEG3TnqFa979vc --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.13.2",
    "@tsconfig/create-react-app": "1.0.3",
    "@types/node": "16.18.71",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.0.0",
    "prettier": "3.0.2",
    "prettier-plugin-organize-imports": "3.2.4",
    "react-scripts": "5.0.1",
    "ts-extras": "0.11.0",
    "typescript": "4.9.5"
  },
  "overrides": {
    "react-scripts": {
      "nth-check": "2.0.1"
    }
  },
  "homepage": "."
}
