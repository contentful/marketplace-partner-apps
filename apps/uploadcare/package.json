{
  "name": "uploadcare-contentful-app",
  "version": "1.0.25",
  "private": true,
  "scripts": {
    "install-ci": "npm ci",
    "lint": "eslint --cache src/**/*.{ts,tsx} && tsc --noEmit",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3Ju8DSs6cEG3TnqFa979vc --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "dependencies": {
    "@contentful/app-sdk": "4.29.3",
    "@contentful/f36-components": "4.74.3",
    "@contentful/f36-icons": "4.29.0",
    "@contentful/f36-tokens": "4.1.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/css": "11.13.5",
    "@uploadcare/blocks": "0.35.2",
    "@uploadcare/upload-client": "6.14.2",
    "contentful-management": "11.40.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.27.0",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/node": "18.15.3",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "cross-env": "7.0.3",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "react-scripts": "5.0.1",
    "ts-extras": "0.11.0",
    "typescript": "5.5.4"
  },
  "overrides": {
    "react-scripts": {
      "resolve-url-loader": "5.0.0",
      "typescript": "5.3.3"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
