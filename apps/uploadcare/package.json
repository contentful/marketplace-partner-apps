{
  "name": "uploadcare-contentful-app",
  "version": "1.0.17",
  "private": true,
  "scripts": {
    "install-ci": "npm ci",
    "lint": "eslint --cache src/**/*.{ts,tsx} && tsc --noEmit",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3Ju8DSs6cEG3TnqFa979vc --token ${CONTENTFUL_CMA_TOKEN}",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@contentful/app-sdk": "4.29.0",
    "@contentful/f36-components": "4.67.4",
    "@contentful/f36-icons": "4.29.0",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/css": "11.13.0",
    "@uploadcare/blocks": "0.35.2",
    "@uploadcare/upload-client": "6.14.1",
    "contentful-management": "11.31.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.25.0",
    "@testing-library/react": "^16.0.0",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/node": "18.15.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "happy-dom": "^14.12.3",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "ts-extras": "0.11.0",
    "typescript": "5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "overrides": {
    "react-scripts": {
      "resolve-url-loader": "5.0.0",
      "typescript": "5.3.3"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
