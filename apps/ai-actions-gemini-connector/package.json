{
  "name": "ai-actions-gemini-connector",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.7",
    "@contentful/f36-components": "4.80.4",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "^11.13.5",
    "@hookform/resolvers": "^5.1.1",
    "@tanstack/react-query": "^5.80.7",
    "@vitejs/plugin-react": "^4.5.2",
    "contentful-management": "^11.53.0",
    "eslint-plugin-mocha": "^11.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.58.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.3"
  },
  "scripts": {
    "lint": "eslint src functions",
    "lint:fix": "eslint src functions --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mjs,mts}\" && npm run lint:fix",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mjs,mts}\" && npm run lint",
    "typecheck": "tsc --noEmit",
    "start": "vite",
    "build": "vite build && npm run build:functions",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:functions": "vitest --config functions/vite.config.mts --run",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "add-locations": "contentful-app-scripts add-locations",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:functions": "contentful-app-scripts build-functions --ci",
    "create-app-action": "node -r dotenv/config create-app-action.js",
    "install-app": "contentful-app-scripts install",
    "open-settings": "contentful-app-scripts open-settings"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-action-utils": "^1.4.1",
    "@contentful/app-scripts": "^2.5.5",
    "@contentful/node-apps-toolkit": "^3.13.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@tsconfig/create-react-app": "2.0.5",
    "@tsconfig/recommended": "1.0.8",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint-plugin-mocha": "^10.4.0",
    "@types/invariant": "^2.2.37",
    "@types/node": "^22.15.31",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitest/coverage-v8": "^3.2.3",
    "cross-env": "7.0.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "invariant": "^2.2.4",
    "prettier": "^3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.34.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.64"
  },
  "homepage": "."
}
