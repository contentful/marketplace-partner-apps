{
  "name": "salesforce-leads",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.78.0",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "10.46.4",
    "emotion": "10.0.27",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx ./src",
    "test": "vitest run",
    "install-ci": "npm ci",
    "coverage": "vitest run --coverage",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.33.2",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.13",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "7.0.3",
    "happy-dom": "17.4.7",
    "tailwindcss": "^3.4.17",
    "typescript": "4.9.5",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0"
  }
}
