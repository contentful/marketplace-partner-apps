{
  "name": "deep-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-components": "file:../../packages/contentful-app-components",
    "@contentful/app-sdk": "^4.24.0",
    "@contentful/f36-components": "4.60.0",
    "@contentful/f36-tokens": "4.0.4",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "10.46.4",
    "emotion": "10.0.27",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "install-ci": "npm install",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.15.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "homepage": "."
}
