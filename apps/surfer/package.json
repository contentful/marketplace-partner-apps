{
  "name": "surfer-contentful-app",
  "version": "1.0.34",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.45.0",
    "@contentful/f36-components": "4.81.1",
    "@contentful/f36-icons": "^4.29.1",
    "@contentful/f36-multiselect": "^4.81.1",
    "@contentful/f36-tokens": "^4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@contentful/rich-text-html-renderer": "^16.6.10",
    "@emotion/css": "^11.13.5",
    "contentful-management": "11.62.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "install-ci": "npm ci",
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 4c6SxDAXoHPLcFmoMLvGde --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.6",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "^20",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "happy-dom": "^20.0.10",
    "typescript": "5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  },
  "homepage": "."
}
