    "@emotion/css": "^11.13.4",
    "contentful-management": "11.35.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "install-ci": "npm ci",
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 4c6SxDAXoHPLcFmoMLvGde --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.30.0",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "18.15.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "happy-dom": "^15.7.4",
