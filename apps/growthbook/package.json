{
  "name": "growthbook-contentful-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install-ci": "npm ci",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --ci",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
