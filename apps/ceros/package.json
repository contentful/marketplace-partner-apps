{
  "name": "ceros-contentful-app",
  "version": "1.0.17",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.28.0",
    "@contentful/f36-components": "4.67.1",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@extractus/oembed-extractor": "^4.0.5",
    "contentful-management": "11.29.0",
    "@emotion/css": "11.11.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "install-ci": "npm ci",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3ts0Y8tta9da2qV0qQSoZG --token ${CONTENTFUL_CMA_TOKEN}",
    "deploy:staging": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${TEST_ORG_ID} --definition-id 4S4r9KtVsjT88RAjrkhuMM --token ${TEST_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.25.0",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "15.0.7",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "18.15.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "cross-env": "7.0.3",
    "react-scripts": "5.0.1",
    "typescript": "^5.5.3"
  },
  "homepage": "."
}
