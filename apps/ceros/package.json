{
  "name": "ceros-contentful-app",
  "version": "1.0.20",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.69.0",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "11.13.0",
    "@extractus/oembed-extractor": "^4.0.5",
    "contentful-management": "11.33.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "install-ci": "npm ci",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3ts0Y8tta9da2qV0qQSoZG --token ${CONTENTFUL_CMA_TOKEN}",
    "deploy:staging": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${TEST_ORG_ID} --definition-id 4S4r9KtVsjT88RAjrkhuMM --token ${TEST_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.27.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "^16.0.1",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "18.15.3",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "happy-dom": "^15.7.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "homepage": "."
}
