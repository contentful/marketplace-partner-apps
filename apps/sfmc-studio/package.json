{
  "name": "sfmc-studio",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "install-ci": "npm ci",
    "dev": "next dev",
    "build": "npm run build-app && npm run fix-paths",
    "build-app": "next build",
    "fix-paths": "node fix-paths.js",
    "clean-paths": "LC_ALL=C.UTF-8 find out -type f -exec sed -i '' -e 's|/_next|./_next|g' {} +",
    "start": "next start",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./next --organization-id ${DEFINITIONS_ORG_ID} --definition-id 4kk0CWKMz2hDS2amuVelsV --token ${CONTENTFUL_CMA_TOKEN}",
    "export": "npx serve out -p 3005",
    "lint": "next lint",
    "test": "jest --ci",
    "test:ci": "jest --ci",
    "jest": {
      "setupFiles": [
        "<rootDir>/jest.setup.js"
      ]
    },
    "coverage": "jest --coverage --silent --json --outputFile=jest-output.json"
  },
  "dependencies": {
    "@ant-design/plots": "^2.3.3",
    "@contentful/app-scripts": "^1.32.2",
    "@contentful/app-sdk": "^4.29.5",
    "@contentful/f36-components": "^4.78.0",
    "@contentful/react-apps-toolkit": "^1.2.16",
    "@reduxjs/toolkit": "^2.6.0",
    "antd": "^5.24.2",
    "axios": "^1.8.1",
    "crypto-js": "^4.2.0",
    "currency-symbol-map": "^5.1.0",
    "dayjs": "^1.11.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "html-react-parser": "^5.2.2",
    "html-to-image": "^1.11.13",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "next": "^15.2.1",
    "prettier": "^3.5.3",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-mock-store": "^1.5.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^14.3.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/redux-mock-store": "^1.5.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "typescript": "^5"
  }
}
