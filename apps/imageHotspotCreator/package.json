{
  "name": "image-hotspot-creator",
  "version": "1.0.24",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.70.0",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@emotion/css": "^11.13.0",
    "@mui/styled-engine-sc": "^5.12.0",
    "axios": "^1.7.7",
    "clone-deep": "^4.0.1",
    "contentful": "^10.15.1",
    "contentful-management": "11.35.0",
    "emotion": "10.0.27",
    "path-browserify": "^1.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-image-crop": "^11.0.7",
    "react-magic-dropzone": "^1.0.1",
    "styled-components": "5.3.11"
  },
  "scripts": {
    "start": "vite",
    "build": "rm -rf build && tsc && vite build",
    "test": "vitest run --passWithNoTests",
    "generate-docs": "jsdoc -c jsdoc.json",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "install-ci": "npm ci",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 3BGvGx9eF1bYGJw7tmHdXv --token ${CONTENTFUL_CMA_TOKEN}",
    "deploy:staging": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${TEST_ORG_ID} --definition-id 5OCsyVbrhu7bJdAJxkbgtq --token ${TEST_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.32.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.0.0",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/jest": "29.5.14",
    "@types/node": "18.15.3",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "better-docs": "^2.7.2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.0",
    "jsdoc": "^4.0.4",
    "typescript": "4.9.5",
    "happy-dom": "^14.12.3",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "homepage": ".",
  "overrides": {
    "better-docs": {
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  }
}
