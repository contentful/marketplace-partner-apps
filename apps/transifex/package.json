{
  "name": "transifex-contentful-app",
  "version": "1.2.6",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.29.1",
    "@contentful/f36-components": "4.74.1",
    "@contentful/f36-icons": "^4.28.2",
    "@contentful/f36-tokens": "4.1.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "^11.13.4",
    "contentful-management": "^11.35.1",
    "eslint": "^9.12.0",
    "nconf": "^0.12.0",
    "prop-type": "^0.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-query": "^3.39.3"
  },
  "scripts": {
    "install-ci": "npm ci",
    "start": "vite",
    "build": "vite build --mode=prod",
    "build:staging": "vite build --mode=staging",
    "build:dev": "vite build --mode=development",
    "lint": "prettier --write ./src && eslint --fix --ext js,jsx ./src",
    "preview": "vite preview",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./dist --organization-id ${DEFINITIONS_ORG_ID} --definition-id b7SVv0k1EYbQRsusviQDi --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "devDependencies": {
    "@contentful/app-scripts": "^1.29.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "18.15.3",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "happy-dom": "14.12.3",
    "prettier": "^3.3.3",
    "typescript": "5.6.3",
    "vite": "5.4.9",
    "vitest": "2.1.3"
  }
}
