{
  "name": "markup-ai-contentful-app",
  "version": "1.1.1",
  "private": false,
  "homepage": ".",
  "type": "module",
  "scripts": {
    "add-locations": "contentful-app-scripts add-locations",
    "build": "npm run type-check && vite build",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "dev": "vite",
    "filter-openapi": "node scripts/filter-openapi.js",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "install-ci": "npm ci",
    "lint": "npm run lint:check",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "openapi-ts": "npm run filter-openapi && openapi-ts",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "type-check": "tsc --noEmit",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 772aa0yka2VSPsAHnvnQ7 --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "2.11.3",
    "@codemirror/lang-html": "6.4.11",
    "@codemirror/lang-markdown": "6.5.0",
    "@codemirror/merge": "6.11.2",
    "@codemirror/state": "6.5.4",
    "@codemirror/view": "6.39.11",
    "@contentful/app-sdk": "4.51.0",
    "@contentful/f36-components": "5.9.0",
    "@contentful/f36-tokens": "5.1.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@tanstack/react-query": "5.90.18",
    "contentful-management": "11.67.2",
    "diff": "8.0.3",
    "dompurify": "3.3.1",
    "i18next": "25.7.4",
    "markdown-it": "14.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@contentful/app-scripts": "2.5.6",
    "@emotion/jest": "11.14.2",
    "@hey-api/openapi-ts": "0.89.2",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.10.1",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "jsdom": "27.4.0",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  }
}