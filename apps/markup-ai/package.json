{
  "name": "markup-ai-contentful-app",
  "version": "0.1.7",
  "private": false,
  "type": "module",
  "dependencies": {
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/merge": "^6.11.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@contentful/app-sdk": "^4.45.0",
    "@contentful/f36-components": "4.81.1",
    "@contentful/f36-tokens": "5.1.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@markupai/toolkit": "0.3.3",
    "contentful-management": "11.62.0",
    "diff": "^8.0.2",
    "dompurify": "^3.3.0",
    "i18next": "^25.6.0",
    "markdown-it": "^14.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "install-ci": "npm ci",
    "build": "npm run type-check && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier . --write",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "type-check": "tsc --noEmit",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "add-locations": "contentful-app-scripts add-locations",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id 772aa0yka2VSPsAHnvnQ7 --token ${CONTENTFUL_CMA_TOKEN}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "^2.5.6",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "10.1.0",
    "eslint": "^9.39.1",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.0",
    "vitest": "^3.2.4"
  },
  "homepage": "."
}
