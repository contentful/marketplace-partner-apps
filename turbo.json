{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^test"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "deploy:staging": {
      "dependsOn": ["build", "test"]
    },
    "deploy": {
      "dependsOn": ["build", "test"]
    }
  }
}
